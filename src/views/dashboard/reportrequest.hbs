<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark">Reporte de Solicitudes Pendientes / Realizadas </h1>
            </div><!-- /.col -->
             <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="/dashboard/">Inicio</a></li>
              <li class="breadcrumb-item active">Reporte de Coordinadores</li>
            </ol>
          </div>
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>

<section class="content">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="card card-primary">
                    <div class="card-header">
                        <h3 class="card-title">Generar Reporte</h3>
                    </div>
                    <form role="form" action="/dashboard/newreportrequest" method="post" >
                        <div class="card-body">
                           
                            <div class="row g-2">
                                <div class="mb-3 col-md-6">
                                    <div class="form-group">
                                        <label for="estado">Estado:</label>
                                       
                                        <select  name="estado" class="form-control form-select" id="cargo">
                                            
                             
                                            <option value="0">PENDIENTES</option>
                                            <option value="1">REALIZADAS</option>
                                          <option   value="2">DECLINADAS</option>
                                        </select>

                                    </div>
                                </div>
                                <div class="mb-3 col-md-6">
                                    <div class="form-group">
                                        <label for="municipio">Municipio:</label>
                                        <select name="municipio"class="form-control form-select" id="municipios">
                                          <option value="0"> </option>
                                         {{#each municipios}}
                                            <option value={{'clvmunicipio'}}>{{'nombremunicipio'}}</option>
                                            {{/each}} 
                                        </select>

                                    </div>
                                </div>
                           
                            </div>

                            <div class="row g-2">
                                <div class="mb-3 col-md-6">
                                    <div class="form-group">
                                       <label for="fecha1" class="col-form-label">FECHA INICIAL:</label>
                                       <input type="date" class="form-control" id="fecha1" name="fecha1">

                                    </div>
                                </div>
                                <div class="mb-3 col-md-6">
                                    <div class="form-group">
                                       <label for="fecha2" class="col-form-label">FECHA FINAL:</label>
                                       <input type="date" class="form-control" id="fecha2" name="fecha2">

                                    </div>
                                </div>
                           
                            </div>
                      
                        </div>
                        <!-- /.card-body -->

                        <div class="card-footer">
                            <button type="submit" class="btn btn-primary" id="agregar">Generar</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>
<script src="/plugins/jquery/jquery.min.js"></script>
<script src="/js/reportemanage.js"></script>
<script src="/js/validatereportrequest.js"></script>