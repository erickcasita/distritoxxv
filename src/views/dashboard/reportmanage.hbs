<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark">Reporte de Coordinadores / Ruteros / Agentes (Subs) Municipales</h1>
            </div><!-- /.col -->
             <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="/dashboard/">Inicio</a></li>
              <li class="breadcrumb-item active">Reporte de Coordinadores</li>
            </ol>
          </div>
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>

<section class="content">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="card card-primary">
                    <div class="card-header">
                        <h3 class="card-title">Generar Reporte</h3>
                    </div>
                    <form role="form" action="/dashboard/newreportmanage" method="post" >
                        <div class="card-body">
                           
                            <div class="row g-3">
                                <div class="mb-3 col-md-4">
                                    <div class="form-group">
                                        <label for="cargo">Cargo:</label>
                                       
                                        <select  name="cargo" class="form-control form-select" id="cargo">
                                            <option value="0"> </option>
                                            {{#each cargos}}
                                            <option value={{'clvcargos'}}>{{'nombrecargo'}}</option>
                                            {{/each}} 
                                        </select>

                                    </div>
                                </div>
                                <div class="mb-3 col-md-4">
                                    <div class="form-group">
                                        <label for="municipio">Municipio:</label>
                                        <select name="municipio"class="form-control form-select" id="municipios">
                                          <option value="0"> </option>
                                         {{#each municipios}}
                                            <option value={{'clvmunicipio'}}>{{'nombremunicipio'}}</option>
                                            {{/each}} 
                                        </select>

                                    </div>
                                </div>
                                <div class="mb-3 col-md-4">
                                    <div class="form-group">
                                        <label for="colonia">Colonia o Comunidad:</label>
                                        <select name="colonia"class="form-control form-select" id="colonia">
                                           
                                        </select>

                                    </div>
                                </div>
                            </div>
                      
                        </div>
                        <!-- /.card-body -->

                        <div class="card-footer">
                            <button type="submit" class="btn btn-primary" id="agregar">Generar</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>
<script src="/plugins/jquery/jquery.min.js"></script>
<script src="/js/reportemanage.js"></script>
